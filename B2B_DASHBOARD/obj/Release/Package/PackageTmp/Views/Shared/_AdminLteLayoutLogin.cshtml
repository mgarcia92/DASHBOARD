<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard De Ventas</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
   <!-- <link rel="stylesheet" href="../../bower_components/Ionicons/css/ionicons.min.css"> -->
    <!-- Theme style -->
    <link href="~/Content/AdminLTE/AdminLTE.min.css" rel="stylesheet" />
    <!-- iCheck -->
    <link href="~/Scripts/AdminLTE/plugins/iCheck/square/blue.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<style type="text/css">
    .imgLogin {
        width: 90px;
        height: 70px;
    }

    .login-box-body{
        background-color:rgba(230,240,230,.2);
        border: 1px solid steelblue;
        box-shadow:5px 2px 4px;
        border-radius:5px;
    }

    .login-logo .title-login{
        color:black;
    }

</style>
<body class="hold-transition " id="body">
    <div class="login-box">
        <div class="login-logo">
            <a href="/Login/" class="title-login">Dashboard <b>Plumrose</b>
                <img src="~/Content/img/plmrose.png" alt="" class="imgLogin"/>
            </a>

        </div>
        <!-- /.login-logo -->
        <div class="login-box-body">
            <p class="login-box-msg">@ViewBag.msg</p>

            @RenderBody()
        </div>
        <!-- /.login-box-body -->
    </div>
    <!-- /.login-box -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/backstretch");
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/AdminLTE/plugins/iCheck/icheck.min.js"></script>
    @Scripts.Render("~/bundles/ScriptLogin")
    <script>
  


    </script>
</body>

</html>